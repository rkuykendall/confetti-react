  },
  "dependencies": {
    "tween-functions": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "np": "^5.0.0",
    "prettier": "^1.19.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "tsdx": "^0.14.1",
    "typescript": "^3.3.3333"
  }
